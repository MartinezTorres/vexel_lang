// Stack implementation.

#Stack(data, top:#i32);

&(s)#Stack::push(item) {
  s.top < |s.data| ? {
    s.data[s.top] = item;
    s.top = s.top + 1;
    0
  } : {
    0
  };
}

&(s)#Stack::clear() {
  s.top = 0
}

&(s)#Stack::size() -> #i32 {
  s.top
}

&(s)#Stack::is_empty() -> #b {
  s.top == 0
}

&(s)#Stack::is_full() -> #b {
  s.top == |s.data|
}
