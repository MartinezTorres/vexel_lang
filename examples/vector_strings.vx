// Vector of strings example
// Demonstrates module imports, vectors, strings, and printing

::lib::print;

#StrVec(data:#s[4], len:#i32, cap:#i32);

&print_vector_strings(v:#StrVec) {
  i:#i32 = 0;
  (i < v.len)@{
    print_str(v.data[i]);
    print_newline();
    i = i + (#i32)1;
  };
}

&^main() -> #i32 {
  // Create a vector of strings
  v:#StrVec = StrVec(["Hello", "World", "from", "Vexel"], 4, 4);

  print_vector_strings(v);

  0
}
