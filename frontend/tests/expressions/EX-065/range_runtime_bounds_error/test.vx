// @rfc: docs/vexel-rfc.md#expressions--control
// @desc: Both bounds of a range must be compile-time constants | Runtime range bounds produce compile error


// EX-065: Runtime bounds error
&!runtime_bound() -> #i32;

&^main() -> #i32 {
    x:#i32 = runtime_bound();
    // ERROR: x is runtime value and not compile-time constant
    r = 0..x;
    -> 0;
}
