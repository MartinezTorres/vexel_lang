// @rfc: docs/vexel-rfc.md#expressions--control
// @desc: Bitwise AND (&) has precedence after additive operators | Bitwise AND (&) has precedence after additive operators


// EX-074: Bitwise AND precedence
&^main() -> #i32 {
    // 8 + 4 & 6 is (8 + 4) & 6
    result:#u32 = (#u32)8 + (#u32)4 & (#u32)6;
    -> result;  // 12 & 6 = 4
}
