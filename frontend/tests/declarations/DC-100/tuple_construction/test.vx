// @desc: Tuple construction uses syntax '"'"'(expr1, expr2, ...)'"'"' to create anonymous struct | Tuple construction syntax


// DC-100: Tuple construction creates anonymous struct
&get_coords() -> (#i32, #i32) {
    (42, 84)
}

&^main() -> #i32 {
    x:#i32;
    y:#i32;
    x, y = get_coords();
    x + y
}
