// DC-077: Iteration methods have exactly one receiver
#Sequence(count:#i32);

&(s)#Sequence::@($loop) {
    _:#i32 = s.count;
    $loop;
}

&^main() -> #i32 {
    seq:#Sequence = Sequence(3);
    seq@{ -> 0; };
    0
}
