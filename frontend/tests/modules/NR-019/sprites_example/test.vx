// @rfc: doc/vexel-rfc.md#name-resolution--modules
// @desc: Example: sprites = ::assets::tiles; embeds contents of each file within assets/tiles/ | Example of embedding directory of sprite files


// NR-019: Example: sprites = ::assets::tiles; embeds contents of each file within assets/tiles/
sprites = ::assets::tiles;

&^main() -> #i32 {
    // Should have 2 sprites, each with name and content
    len:#i32 = |sprites|;
    result:#i32 = (len == 2) ? (#i32)0 : (#i32)1;
    -> result;
}
