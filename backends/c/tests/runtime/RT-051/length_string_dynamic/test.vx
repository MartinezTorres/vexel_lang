// RT-051: Length operator |expr| on strings is allowed dynamically

&get_string(flag:#b) -> #s {
    flag ? "hello" : "world"
}

&^main(flag:#b) -> #i32 {
    s:#s = get_string(flag);
    |s|  // Dynamic length
}
