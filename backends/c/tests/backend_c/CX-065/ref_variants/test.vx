// @rfc: docs/vexel-rfc.md#annotations
// @desc: Ref variants specialize mutable vs immutable receivers | Ref variants emit separate symbols
// @expect-exit: 0
// @command: python3 -c "import pathlib, re, subprocess, sys; subprocess.run([sys.argv[1], '-b', 'c', 'test.vx'], check=True); src = pathlib.Path('out.c').read_text(); ok = re.search(r'\\bvx_Box__get\\b', src) and re.search(r'\\bvx_Box__get__refN\\b', src); sys.exit(0 if ok else 1)" {VEXEL_C}

#Box(val:#i32);

const_box:#Box = Box(2);
mut bump:#i32 = 0;

&(self)#Box::get() -> #i32 {
    self.val
}

&^main() -> #i32 {
    a:#Box = Box(1);
    a.get() + const_box.get() + bump
}
